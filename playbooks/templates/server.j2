[Interface]
PrivateKey = {{ genkey.stdout }}
Address = {{ nodes[inventory_hostname] }}
ListenPort = {{ ListenPort }}

{% for host in pubkey_list.results %}
{% if host.item != inventory_hostname %}
[Peer]
# {{ host.item }}
PublicKey = {{ host.stdout }}
AllowIPS = {{ nodes[host.item] }}
Endpoint = {{ hostvars[host.item].ansible_host }}:{{ ListenPort }}
{% endif %}
{% endfor %}